{{template "header" .}}
<body class="body">
<div class="container-fluid vh-100">
    <div class="row" id="row1">
        <img id="headlineImg" src="/static/image/headliner.jpg" alt="headline image">
        <img class="alphaImg" src="/static/image/alpha.svg" alt="alpha">
        <div id="fade"></div>
        <div class="col-sm-1"></div>
        <div class="col-sm-10">
            <div id="headlineDiv">
                <a href="/" rel="noreferrer">
                    <img id="bannerImg" class="clickable cursorPointer" src="/static/image/yourplace-banner-title.svg" alt="logo">
                </a>
                <p id="subtitleText">Download</p>
            </div>
        </div>
        <div class="col-sm-1">
            {{template "htmlMenu" .}}
        </div>
    </div>
</div>
<input type="hidden" id="csrfToken" value="{{ .csrfToken }}">
</body>
</html>